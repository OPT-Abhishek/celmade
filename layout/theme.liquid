<!doctype html>
<html class="no-js supports-no-cookies {% if settings.enable_box %}box-layout {% endif %}" lang="{{ shop.locale }}" dir="{%- if settings.enable_rtl -%}rtl{%- else -%}ltr{%- endif -%}">
<head>
  {% render 'header_file' %}
  {% render 'font-css' %}
  {% render 'yoast-seo' %}
  <!-- Google tag (gtag.js) -->
  <script type="lazyload_int" data-src="https://www.googletagmanager.com/gtag/js?id=G-S1KP6ZXXHM"></script>
  <script type="lazyload_int">
    window.dataLayer = window.dataLayer || [];
    function gtag(){dataLayer.push(arguments);}
    gtag('js', new Date());
  
    gtag('config', 'G-S1KP6ZXXHM');
  </script>
  
  <meta name="facebook-domain-verification" content="zi2glcdafswqiyxyyho9pwx38ohif0" />

  <!-- Google Tag Manager -->
  <script type="lazyload_int">(function(w,d,s,l,i){w[l]=w[l]||[];w[l].push({'gtm.start':
  new Date().getTime(),event:'gtm.js'});var f=d.getElementsByTagName(s)[0],
  j=d.createElement(s),dl=l!='dataLayer'?'&l='+l:'';j.async=true;j.src=
  'https://www.googletagmanager.com/gtm.js?id='+i+dl;f.parentNode.insertBefore(j,f);
  })(window,document,'script','dataLayer','GTM-WLS85SRL');</script>
  <!-- End Google Tag Manager -->
  
  <!-- Basic and Helper page needs -->
  <meta charset="utf-8">
  <meta http-equiv="x-ua-compatible" content="IE=edge">
  <meta name="viewport" content="width=device-width,initial-scale=1">
  <meta name="theme-color" content="var(--font-color-primary)">
  
  
    <link rel="preconnect" href="https://cdn.shopify.com" crossorigin>
  
    {%- unless settings.body_custom_font_handle_enable and settings.header_custom_font_handle_enable and settings.section_header_custom_font_handle_enable and settings.paragraph_custom_font_handle_enable and settings.other_custom_font_handle_enable -%}
    <link rel="preconnect" href="https://fonts.shopifycdn.com" crossorigin>
    {%- endunless -%}

    {%- liquid
    unless settings.body_custom_font_handle_enable
        assign body_font = settings.body_font_handle
        assign body_font_bolder = body_font | font_modify: 'weight', 'bolder'
        assign body_font_bold = body_font | font_modify: 'weight', 'bold'
        assign body_font_italic = body_font | font_modify: 'style', 'italic'
        assign body_font_bold_italic = body_font_bold | font_modify: 'style', 'italic'
    endunless
    
    unless settings.header_custom_font_handle_enable
        assign header_font = settings.header_font_handle
    endunless
    
    unless settings.section_header_custom_font_handle_enable
        assign section_header_font = settings.section_header_font_handle
    endunless
    
    unless settings.paragraph_custom_font_handle_enable
        assign paragraph_font = settings.paragraph_font_handle
    endunless
    
    unless settings.other_custom_font_handle_enable
        assign other_font = settings.other_font_handle
    endunless
    -%}

    <!-- CSS -->
    {%- if canonical_url != blank -%}
    <link rel="canonical" href="{{ canonical_url }}">
    {%- endif -%}

    {%- if settings.favicon != blank -%}
    <link rel="shortcut icon" href="{{ settings.favicon | image_url }}" type="image/png" />
    {%- endif -%}

    <!-- Title and description -->
    {% capture seo_title %}
    {{ page_title }}
    {% if current_tags %}
    {%- assign meta_tags = current_tags | join: ', ' %} &ndash; {{ 'general.meta.tags' | t: tags: meta_tags -}}
    {% endif %}
    {% if current_page != 1 %}
    &ndash; {{ 'general.meta.page' | t: page: current_page }}
    {% endif %}
    {% unless page_title contains shop.name %}
    &ndash; {{ shop.name }}
    {% endunless %}
    {% endcapture %}
    {%- comment -%}Auto-disabled. Yoast plugin is taking care of SEO output.{%- endcomment -%}
    {%- if disabled_by_yoast_seo -%}
    <title>{{ seo_title }}</title>
    {%- comment -%} End Yoast auto-disable. {%- endcomment -%}
    {%- endif -%}

    {%- if page_description -%}
    {%- comment -%}Auto-disabled. Yoast plugin is taking care of Meta Tags output.{%- endcomment -%}
      {%- if disabled_by_yoast_seo -%}
      <meta name="description" content="{{ page_description | escape }}">
      {%- comment -%} End Yoast auto-disable. {%- endcomment -%}
      {%- endif -%}
    {%- endif -%}
    
    <!-- Helpers -->
    {% render 'social-meta-tags' %}


    {% if template contains 'index' or template contains 'collection' or template contains 'product' %}
		{%- render 'font-main' -%}
	{% else %}
		{%- render 'font-other' -%}
	{% endif %}
  
    <!-- CSS -->
    <style>
    
    {%- liquid
    unless settings.header_custom_font_handle_enable
      assign header_font = settings.header_font_handle
     endunless 
    -%}
      
    {%- liquid
      unless settings.section_header_custom_font_handle_enable
        assign section_header_font = settings.section_header_font_handle
      endunless
    -%}
    
    {%- liquid
      unless settings.paragraph_custom_font_handle_enable
      assign paragraph_font = settings.paragraph_font_handle
      endunless
    -%}
    
    {%- liquid
      unless settings.other_custom_font_handle_enable
        assign other_font = settings.other_font_handle
      endunless
    -%}
    
    {%- liquid
      unless settings.body_custom_font_handle_enable
        assign body_font = settings.body_font_handle
        assign body_font_bolder = body_font | font_modify: 'weight', 'bolder'
        assign body_font_bold = body_font | font_modify: 'weight', 'bold'
        assign body_font_italic = body_font | font_modify: 'style', 'italic'
        assign body_font_bold_italic = body_font_bold | font_modify: 'style', 'italic'
      endunless
    -%}
    </style>

    {% if template contains "index" %}
		{% render "index-css" %}		
	{% endif %}

    {% if template contains "collection" %}
	   {% render "collection-css" %}		
	{% endif %}

    {% if template contains "product" %}
		{% render "product-css" %}		
	{% endif %}

  	{% if template contains 'index' or template contains 'collection' or template contains 'product' %}
		{%- render 'header-css' -%}
	{% else %}
		{%- render 'header-css2' -%}
	{% endif %}
  
    {%- render 'css-variables' -%}
    <style>
    {%- liquid 
      unless settings.header_custom_font_handle_enable
        echo header_font | font_face: font_display: 'swap'
      endunless
    -%}

    {% unless settings.section_header_custom_font_handle_enable %}
    {{ section_header_font | font_face: font_display: 'swap' }}
    {% endunless %}

    {% unless settings.paragraph_custom_font_handle_enable %}
    {{ paragraph_font | font_face: font_display: 'swap' }}
    {% endunless %}

    {% unless settings.other_custom_font_handle_enable %}
    {{ other_font | font_face: font_display: 'swap' }}
    {% endunless %}

    {% unless settings.body_custom_font_handle_enable %}
    {{ body_font | font_face: font_display: 'swap' }}
    {{ body_font_bolder | font_face: font_display: 'swap' }}
    {{ body_font_bold | font_face: font_display: 'swap' }}
    {{ body_font_italic | font_face: font_display: 'swap' }}
    {{ body_font_bold_italic | font_face: font_display: 'swap' }}
    {% endunless %}
    :root {
      --font-weight-body--bold: {{ body_font_bold.weight | default: 700 }};
      --font-weight-body--bolder: {{ body_font_bolder.weight | default: 700 }};
    
      --font-color-body: {{ settings.body_text_color }};
      --font-size-body: {{ settings.body_font_size }}px;
      --background-color-body: {{ settings.body_bgcolor }};
      --extra-color-body: {{ settings.extra_bgcolor }};
      {% if settings.body_custom_font_handle_enable %}
      --font-custom-stack-body: '{{ settings.body_custom_font_handle }}', sans-serif;
      --font-custom-weight-body: {{ settings.body_custom_font_weight }};
      --font-custom-style-body: {{ settings.body_custom_font_style }};
      {% else %}
      --font-stack-body: {{ body_font.family }}, {{ body_font.fallback_families }};
      --font-weight-body: {{ body_font.weight }};
      --font-style-body: {{ body_font.style }};
      {% endif %}
      --font-letter-spacing-body: {{ settings.body_letter_spacing }}px;
    
      --font-color-header: {{ settings.heading_text_color }};
      {% if settings.header_custom_font_handle_enable %}
      --font-custom-stack-header: '{{ settings.header_custom_font_handle }}', sans-serif;
      --font-custom-weight-header: {{ settings.header_custom_font_weight }};
      --font-custom-style-header: {{ settings.header_custom_font_style }};
      {% else %}
      --font-stack-header: {{ header_font.family }}, {{ header_font.fallback_families }};
      --font-weight-header: {{ header_font.weight }};
      --font-style-header: {{ header_font.style }};
      {% endif %}
    
      --font-color-section-header: {{ settings.section_heading_text_color }};
      {% if settings.section_header_custom_font_handle_enable %}
      --font-custom-stack-section-header: '{{ settings.section_header_custom_font_handle }}', sans-serif;
      --font-custom-weight-section-header:{{ settings.section_header_custom_font_weight }};
      --font-custom-style-section-header: {{ settings.section_header_custom_font_style }};
      {% else %}
      --font-stack-section-header: {{ section_header_font.family }}, {{ section_header_font.fallback_families }};
      --font-weight-section-header: {{ section_header_font.weight }};
      --font-style-section-header: {{ section_header_font.style }};
      {% endif %}
      --font-text-section-header: {{ settings.section_header_text_transform }};
    
      --font-color-paragraph: {{ settings.para_text_color }};
      {% if settings.paragraph_custom_font_handle_enable %}
      --font-custom-stack-paragraph: '{{ settings.paragraph_custom_font_handle }}', sans-serif;
      --font-custom-weight-paragraph: {{ settings.paragraph_custom_font_weight }};
      --font-custom-style-paragraph: {{ settings.paragraph_custom_font_style }};
      {% else %}
      --font-stack-paragraph: {{ paragraph_font.family }}, {{ paragraph_font.fallback_families }};
      {% endif %}
      --font-line-height-paragraph: {{ settings.paragraph_font_line_height }}px;
    
      {% if settings.other_custom_font_handle_enable %}
      --font-custom-stack-other: '{{ settings.other_custom_font_handle }}', sans-serif;
      {% else %}
      --font-stack-other: {{ other_font.family }}, {{ other_font.fallback_families }};
      {% endif %}
    
      --font-color-primary: {{ settings.color_primary }};
      --font-color-secondary: {{ settings.color_secondary }};
      --theme-border-color: {{ settings.theme_border_color | color_modify: 'alpha', settings.theme_border_color_opacity }};
    
      --sti-color: {{ settings.tag_color }};
      --sti-font-size: {{ settings.tag_font_size }}px;
      --sti-bgcolor: {{ settings.tag_bgcolor }};
      --sti-text: {{ settings.tag_text_transform }};
      --sti-font-weight: {{ settings.tag_font_weight }};
      --sti-border: 1px solid {{ settings.tag_border_color | color_modify: 'alpha', settings.tag_border_color_opacity }};
      --sti-border-radius: {{ settings.tag_border_radius }}px;
    
      --box-body-bgcolor: {{ settings.box_body_bgcolor }};
    }
  </style>
  
    {%- if request.design_mode or template contains 'cart' or template contains 'checkout' -%}
            {{ content_for_header }}
    {%- else -%}
            {% capture content_for_header2 %}{{ content_for_header }}{% endcapture %}
            {{ content_for_header2 | replace: ' defer="defer" ', ' type="lazyload_int" ' | replace: " src=", " data-src="| replace: "DOMContentLoaded", "w3-DOMContentLoaded" | replace: "window.addEventListener('load', asyncLoad, false);", "window.addEventListener('w3-DOMContentLoaded', asyncLoad, false);" | replace: "addEventListener('load', prefetchAssets);", "addEventListener('w3-DOMContentLoaded', prefetchAssets);" }}
            <script>var trekkie=[];trekkie.integrations=!0;window.BOOMR={},window.BOOMR.version=true;</script>
    {%- endif -%}
  
  {%- render 'header-js' -%}

  {% if template contains 'customers' or 'cart' %}
  <script type="lazyload_int" data-src="{{ 'shopify_common.js' | shopify_asset_url }}"></script>
  {% endif %}
  
  {% render 'custom-css' %}
  {% render 'app-block' %}
{% include 'store-credit-launcher' %}
</head>

<body  id="{{ page_title | handle }}" class="{% if customer %}customer-logged-in {% endif %}template-{{ template.name | handle }}">
    <!-- Google Tag Manager (noscript) -->
    <noscript><iframe src="https://www.googletagmanager.com/ns.html?id=GTM-WLS85SRL"
    height="0" width="0" style="display:none;visibility:hidden"></iframe></noscript>
    <!-- End Google Tag Manager (noscript) -->
  
  {% render 'slow-window' %}
  
  {%- liquid
    if template == 'index' and settings.newsletter_popup_enable
    render 'newsletter-popup'
    endif
  -%}
  
  {% section 'top-nocification-bar' %}
  {% section 'header' %}

  {%- if template != 'index' and template != 'product' -%}
    {% render 'breadcrumb-banner' %}
  {%- endif -%}
  
  <main role="main">
    {{ content_for_layout }}
  </main>
  
  {% render 'fullscreen-bg' %}

  {% liquid section 'our-services' %}
  {% section 'footer' %}

  {% section 'copy-right' %}
  {% render 'back-to-top' %}
  <!-- Necessary JS -->
  {% render 'footer-js' %}
  
  {% render 'footer_file' %}
  {% render 'footer-app-block' %}
{% include 'store-credit-launcher' %}
</body>
</html>